


<WifiWindow>:
    name: "wifi"

    ssid: ssid
    password: passw

    FloatLayout:
        cols:1
        canvas.before:
            Color:
                rgba: 0.09, 0.17, 0.34, 1
            Rectangle:
                # self here refers to the widget i.e BoxLayout
                pos: self.pos
                size: self.size
        FloatLayout:
            size: root.width, root.height/2

            Label:
                text: "Connect to Wifi"
                size_hint: 0.7, 0.15
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.4,0.10
                pos_hint: {"x":0, "top":0.85}
                text: "SSID: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.35, "top":0.85}
                size_hint: 0.4, 0.12
                id: ssid
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Button:
                background_color: (1.0, 1.0, 1.0, 0.0)
                size_hint: 0.4, 0.12
                pos_hint: {'x':0.35, 'top':0.85}
                text: ""
                on_release: root.ssid_focus()

            Label:
                size_hint: 0.4,0.10
                pos_hint: {"x":0, "top":0.7}
                text: "Password: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.35, "top":0.7}
                size_hint: 0.4, 0.12
                id: passw
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

            Button:
                background_color: (1.0, 1.0, 1.0, 0.0)
                size_hint: 0.4, 0.12
                pos_hint: {'x':0.35, 'top':0.7}
                text: ""
                on_release: root.passwd_focus()

            #First Row

            Button:
                pos_hint:{'x':0.15, 'y':0.3}
